# VeneerVision AI S3 Handler Environment Variables
# Copy this file to .env and fill in your actual values

# Required: Your Replicate API token
# Get this from https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Optional: AWS Region (defaults to us-east-1)
AWS_REGION=us-east-1

# Optional: S3 Bucket name (defaults to veneer-vision-site-vvjune28)
S3_BUCKET_NAME=veneer-vision-site-vvjune28

# Optional: Lambda function name (defaults to veneer-s3-handler)
LAMBDA_FUNCTION_NAME=veneer-s3-handler

# Optional: IAM Role ARN (update in deploy.sh)
LAMBDA_ROLE_ARN=arn:aws:iam::YOUR_ACCOUNT_ID:role/veneer-lambda-role 